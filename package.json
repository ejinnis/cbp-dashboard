{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "npx unocss \"app/**/*.{ts,tsx}\" -o ./app/styles/uno.css",
    "deploy": "wrangler publish",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch",
    "dev:css": "unocss \"app/**/*.{ts,tsx}\" -o ./app/styles/uno.css --watch",
    "dev": "remix build && run-p \"dev:*\"",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js",
    "format": "prettier --write \"app/**/*.{ts,tsx}\"",
    "lint": "eslint \"app/**/*.{ts,tsx}\" --fix",
    "postinstall": "npx patch-package"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@remix-run/cloudflare": "^1.7.4",
    "@remix-run/cloudflare-workers": "^1.7.4",
    "@remix-run/react": "^1.7.4",
    "@unocss/reset": "^0.46.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "framer-motion": "^7.6.4",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flip-toolkit": "^7.0.17",
    "react-hotkeys-hook": "^4.0.3",
    "react-icons": "^4.6.0",
    "react-text-transition": "^3.0.2",
    "recharts": "^2.1.16",
    "recoil": "^0.7.6",
    "smart-timeout": "^2.7.1",
    "superjson": "^1.11.0",
    "use-local-storage-state": "^18.1.1",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "@remix-run/dev": "^1.7.4",
    "@remix-run/eslint-config": "^1.7.4",
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "@types/lodash": "^4.14.188",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@unocss/preset-web-fonts": "^0.46.3",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "miniflare": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.5.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "unocss": "^0.46.1",
    "unocss-preset-scrollbar": "^0.2.1",
    "wrangler": "^2.0.22"
  },
  "engines": {
    "node": ">=16.13"
  }
}
